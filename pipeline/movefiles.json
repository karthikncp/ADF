{
	"name": "movefiles",
	"properties": {
		"activities": [
			{
				"name": "input",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference",
						"parameters": {
							"pp_container": {
								"value": "@pipeline().parameters.db_container",
								"type": "Expression"
							},
							"pp_dir": {
								"value": "@pipeline().parameters.db_input",
								"type": "Expression"
							},
							"pp_filename": {
								"value": "@pipeline().parameters.db_filename",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"db_container": {
				"type": "string"
			},
			"db_input": {
				"type": "string"
			},
			"db_filename": {
				"type": "string"
			},
			"db_output": {
				"type": "string"
			},
			"db_archieve": {
				"type": "string"
			}
		},
		"folder": {
			"name": "project1"
		},
		"annotations": []
	}
}